Algoritmo "Vetor"
Var
    Nomes: Vetor[0..4] de Caractere
Inicio
     Nomes[0] <- "Joao"
     Nomes[1] <- "Maria"
     Nomes[2] <- "Jose"
     Nomes[3] <- "Ana"
     Nomes[4] <- "Jorge"
     
     Escreval(Nomes[0])
     Escreval(Nomes[1])
     Escreval(Nomes[2])
     Escreval(Nomes[3])
     Escreval(Nomes[4])


Fimalgoritmo